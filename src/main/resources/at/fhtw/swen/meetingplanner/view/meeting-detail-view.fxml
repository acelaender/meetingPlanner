<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox style="-fx-border-color: #c7c7c7; -fx-border-radius: 10;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.fhtw.swen.meetingplanner.controller.MeetingDetailController">
   <children>
      <Label fx:id="titleLabel" text="Label" />
      <Label fx:id="agendaLabel" text="Label" />
      <Button fx:id="addNoteButton" mnemonicParsing="false" onAction="#onAddNoteButtonClick" text="Add Note" />
      <VBox fx:id="formContainer" prefHeight="200.0" prefWidth="100.0" />
      <VBox fx:id="noteContainer" prefHeight="200.0" prefWidth="100.0" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <VBox.margin>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </VBox.margin>
</VBox>
