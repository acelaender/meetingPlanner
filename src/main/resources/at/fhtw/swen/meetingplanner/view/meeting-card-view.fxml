<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.fhtw.swen.meetingplanner.controller.MeetingCardViewController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="editButton" mnemonicParsing="false" text="Edit" />
                  <Button fx:id="deleteButton" mnemonicParsing="false" text="Delete" />
               </children>
            </HBox>
            <Label fx:id="titleLabel" text="Label" />
            <Label fx:id="timeLabel" text="Label" />
            <Label fx:id="agendaLabel" text="Label" />
            <Button fx:id="expandMeetingButton" mnemonicParsing="false" text="Details" />
         </children>
      </VBox>
   </children>
</VBox>
